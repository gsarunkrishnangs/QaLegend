<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="QaLegend Suite">
	<groups>
		<run>
			<include name="Regression" />
		</run>
	</groups>
	<listeners>
		<listener class-name="com.qalegend.listeners.AnnotationTransformer"/>
	</listeners>
	<test name="QaLegend Test">
		<parameter name="browser" value="Chrome" />
		<parameter name="baseurl"
			value="https://qalegend.com/billing/public/login" />
		<classes>
			<class name="com.qalegend.tests.HomePageTest" />
			<class name="com.qalegend.tests.ResetPasswordPageTest" />
		</classes>
	</test>
</suite> 
