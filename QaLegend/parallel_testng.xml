<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="QaLegend Suite" parallel="tests" thread-count="3">
	<listeners>
		<listener class-name="com.qalegend.listeners.AnnotationTransformer" />
	</listeners>
	<test name="LoginPage Test">
		<parameter name="browser" value="Chrome" />
		<parameter name="baseurl"
			value="https://qalegend.com/billing/public/login" />
		<classes>
			<class name="com.qalegend.tests.LoginPageTest" />
		</classes>
	</test>
	<test name="HomePage Test">
		<parameter name="browser" value="Chrome" />
		<parameter name="baseurl"
			value="https://qalegend.com/billing/public/login" />
		<classes>
			<class name="com.qalegend.tests.HomePageTest" />
		</classes>
	</test>
	<test name="ResetPage Test">
		<parameter name="browser" value="Chrome" />
		<parameter name="baseurl"
			value="https://qalegend.com/billing/public/login" />
		<classes>
			<class name="com.qalegend.tests.ResetPasswordPageTest" />
		</classes>
	</test>
</suite> 
