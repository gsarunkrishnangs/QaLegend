<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="QaLegend Suite">
	<listeners>
		<listener class-name="com.qalegend.listeners.AnnotationTransformer"/>
	</listeners>
	<test name="Edge Test">
		<parameter name="browser" value="Edge" />
		<parameter name="baseurl"
			value="https://qalegend.com/billing/public/login" />
		<classes>
			<class name="com.qalegend.tests.HomePageTest" />
			<class name="com.qalegend.tests.ProductsPageTest" />
		</classes>
	</test>
</suite> 
